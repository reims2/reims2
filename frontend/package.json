{
  "name": "reims2-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "dev": "vite",
    "dev:local": "VITE_API_URL=http://localhost:9966/ vite",
    "dev:remote": "VITE_API_URL=https://dev.reims2.app/ vite",
    "build": "vue-tsc --noEmit && vite build",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "test": "vitest run --coverage",
    "test:watch": "vitest --ui --coverage"
  },
  "engines": {
    "node": "24.13.1"
  },
  "dependencies": {
    "@fontsource-variable/roboto-flex": "5.2.8",
    "@vueuse/core": "14.1.0",
    "axios": "1.13.5",
    "dayjs": "1.11.19",
    "jwt-decode": "4.0.0",
    "pinia": "3.0.4",
    "pinia-plugin-persistedstate": "4.7.1",
    "vue": "3.5.28",
    "vue-router": "5.0.2",
    "vue-toastification": "2.0.0-rc.5",
    "vue3-enter-to-tab": "3.1.6",
    "vuetify": "3.11.7"
  },
  "devDependencies": {
    "@babel/types": "7.29.0",
    "@eslint/js": "9.39.2",
    "@mdi/js": "7.4.47",
    "@pinia/testing": "1.0.3",
    "@testing-library/vue": "8.1.0",
    "@types/eslint": "9.6.1",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.13",
    "@vitejs/plugin-vue": "6.0.4",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "browserslist": "4.28.1",
    "browserslist-to-esbuild": "2.1.1",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-vue": "10.7.0",
    "globals": "17.3.0",
    "happy-dom": "20.6.1",
    "prettier": "3.8.1",
    "sass": "1.97.3",
    "sass-embedded": "1.97.3",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0",
    "unplugin-auto-import": "21.0.0",
    "vite": "7.3.1",
    "vite-plugin-pwa": "1.2.0",
    "vite-plugin-vue-devtools": "8.0.6",
    "vite-plugin-vuetify": "2.1.3",
    "vitest": "4.0.18",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "3.2.4",
    "workbox-window": "7.4.0"
  },
  "resolutions": {
    "vue": "3.5.28"
  }
}
